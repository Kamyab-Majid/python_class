    "def Time_format(time):\n",
    "    hour = int(time[0:2])\n",
    "    am_pm = time[-2:]\n",
    "    if am_pm == 'AM':\n",
    "        output = ('output',time[0:8])\n",
    "    if am_pm == 'PM':\n",
    "        hour+=12\n",
    "        output = ('output',f'{hour}:{time[3:8]}')\n",
    "    print(output)\n",
    "\n",
    "\n",
    "    \n",
    "my_time='01:05:45PM'\n",
    "Time_format(5)"
